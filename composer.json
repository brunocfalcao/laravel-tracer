{
    "name": "brunocfalcao/laravel-tracer",
    "description": "Laravel Tracer - Trace visits, application logs, geolocation, etc",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/laravel-tracer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "brunocfalcao/cerebrus": "dev-master",
        "jaybizzle/crawler-detect": "^1.2",
        "jenssegers/agent": "^2.6",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brunocfalcao\\LaravelTracer\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brunocfalcao\\LaravelTracer\\LaravelTracerServiceProvider"
            ],
            "aliases": {
                "Tracer": "Brunocfalcao\\LaravelTracer\\Facades\\LaravelTracer",
                "Referrer": "Brunocfalcao\\LaravelTracer\\Facades\\Referrer"
            }
        }
    }
}
